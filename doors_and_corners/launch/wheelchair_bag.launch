<launch>
 
  <node pkg="rosbag" type="play" name="player" output="screen" args="-r 0.3 $(find doors_and_corners)/rosbags/2020-11-19-16-22-13.bag"/>

  <node pkg="laser_filters" type="scan_to_scan_filter_chain" output="screen" name="laser_filter">
    <rosparam command="load" file="$(find doors_and_corners)/configs/my_laser_config.yaml" />
  </node>

  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find doors_and_corners)/rviz/wheelchairbag.rviz" />
</launch>
